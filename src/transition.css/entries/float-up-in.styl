@import '../../vars.styl'
@import '../../basic.styl'

+keyframes(ldt-float-up-in)
  0%
    opacity: 0
    transform: translate(0,30%)
  50%
    opacity: 1
  100%
    opacity: 1
    transform: translate(0,0%)

.ldt-float-up-in
  transform: translate(0,30%)
  opacity: 0
  animation(ldt-float-up-in 1s forwards)

