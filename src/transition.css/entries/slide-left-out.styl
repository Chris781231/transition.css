@import '../../vars.styl'
@import '../../basic.styl'

+keyframes(ldt-slide-left-out)
  0%
    transform: translate(0,0)
  10%
    opacity: 1
  60%
    opacity: 0
  100%
    transform: translate(-200%,0)
    opacity: 0
.ldt-slide-left-out
  transform: translate(0,0)
  animation(ldt-slide-left-out 1s cubic-bezier(0,0.5,0.5,1) forwards)
