@import '../../vars.styl'
@import '../../basic.styl'

+keyframes(ldt-float-px-left-in)
  0%
    opacity: 0
    transform: translate(30px,0)
  50%
    opacity: 1
  100%
    opacity: 1
    transform: translate(0px,0)

.ldt-float-px-left-in
  transform: translate(30px,0)
  opacity: 0
  animation(ldt-float-px-left-in 1s forwards)

